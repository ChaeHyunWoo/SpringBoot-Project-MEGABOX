            var idx = $(this).parent().index();

            $.ajaxMegaBox({
                url: '/on/coc/FileUpload/deleteFileUpload.do',
                data: JSON.stringify({
                    fileNo: $(this).data('no'),
                    fileSn: $(this).data('sn'),
                    physicFileDelYn: 'Y'
                }),
                success: function (d) {
                    if (Number(d.deleteCnt) > 0) {
                        $('#imgList p').eq(idx).remove();
                    }
                }
            });
        });
    }
